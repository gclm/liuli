{
    "name": "wechat_demo",
    "author": "liuli_team",
    "collector": {
        "wechat_sougou": {
            "collect_config": {
                "wechat_list": [
                    "老胡的储物柜"
                ],
                "delta_time": 5,
                "spider_type": "playwright"
            }
        }
    },
    "processor": {
        "before_start": [],
        "after_collect": [{
            "func": "to_rss"
        }, {
            "func": "ads_tag",
            "cos_value": 0.6
        }],
        "before_backup_save": [{
            "func": "str_replace",
            "before_str": "data-src=\"",
            "after_str": "src=\"https://images.weserv.nl/?url="
        }]
    },
    "sender": {
        "sender_list": ["wecom", "dings"]
    },
    "backup": {
        "backup_list": ["github", "mongodb"],
        "query_days": 1,
        "delta_time": 5,
        "init_config": {}
    },
    "schedule": {
        "period_list": [
            "00:10",
            "12:10",
            "21:10"
        ]
    }
}